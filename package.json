{
  "name": "xpyjs-theme",
  "displayName": "XpyJs Black",
  "publisher": "xpyjs",
  "description": "A high-contrast dark VS Code theme tailored for JavaScript and TypeScript workflows.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xpyjs/vsc-theme"
  },
  "keywords": [
    "dark",
    "theme",
    "javascript",
    "typescript",
    "contrast"
  ],
  "icon": "images/icon.png",
  "contributes": {
    "themes": [
      {
        "label": "XpyJs Black",
        "uiTheme": "vs-dark",
        "path": "./themes/xpyjs-theme-black.json"
      }
    ]
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "vsce": "^2.24.0"
  }
}